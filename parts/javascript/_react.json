{
    "rn-component": {
        "prefix": "rn-component",
        "description": "Scaffold a React component",
        "body": [
            "import React, {Component} from 'react';",
            "import {$1} from 'react-native';",
            "",
            "export default class $1 extends Component {",
            "constructor(props){",
            "super(props)",
            "this.setState({$2})",
            "}",
            "",
            "render(){",
            "$3",
            "}",
            "}"
        ]
    },
    "rn-stylesheet": {
        "prefix": "rn-stylesheet",
        "description": "Scaffold a StyleSheet for React native",
        "body": [
            "const styles = StyleSheet.create({})"
        ] 
    },
    "import-rn": {
        "prefix": "import-rn",
        "description": "Imports react-native into your file",
        "body": [
            "import {$1} from 'react-native"
        ] 
    }
}